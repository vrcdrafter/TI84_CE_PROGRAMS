<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="DEMO" />
		<Option makefile="makefile" />
		<Option makefile_is_custom="1" />
		<Option execution_dir="C:/CEdev/template/DEMO" />
		<Option pch_mode="2" />
		<Option compiler="cedev_toolchain" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/DEMO" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="cedev_toolchain" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<MakeCommands>
					<Build command="$make -f $makefile PARAM=$(PROJECT_NAME)" />
					<CompileFile command="$make -f $makefile $file" />
					<Clean command="$make -f $makefile clean PARAM=$(PROJECT_NAME)" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="$make -f $makefile debug" />
					<SilentBuild command="$make -f $makefile PARAM=$(PROJECT_NAME) &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/DEMO" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="cedev_toolchain" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
				<MakeCommands>
					<Build command="$make -f $makefile PARAM=$(PROJECT_NAME)" />
					<CompileFile command="$make -f $makefile $file" />
					<Clean command="$make -f $makefile clean PARAM=$(PROJECT_NAME)" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="$make -f $makefile debug" />
					<SilentBuild command="$make -f $makefile PARAM=$(PROJECT_NAME) &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Weverything" />
		</Compiler>
		<ExtraCommands>
			<Add after="zip -j9 zip\$(PROJECT_NAME)_$(TODAY).zip src\*.c src\*.h bin\*.8xp" />
		</ExtraCommands>
		<Unit filename="src/main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions />
	</Project>
</CodeBlocks_project_file>
